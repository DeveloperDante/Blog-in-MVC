@using Blog.Models

@model IEnumerable<Temp>

@{
    ViewBag.Title = "ListAllPosts";
}
@{
    Layout = "~/Views/Shared/_LayoutPageAdmin.cshtml";
}
<h2 class="page-header">All Posts</h2>

<div class="row">
    <div class="col-md-4">
        <ul>
            <li>@Html.ActionLink("Posts", "ListAllPosts", "Admin")</li>
            <li>@Html.ActionLink("Users", "ListAllUsers", "Admin")</li>
        </ul>
    </div>
    <div class="col-md-8">
        <ul>
            @foreach (Temp post in Model)
            {
                <li>@Html.DisplayFor(modelItem => post.PostName)</li>
            }
        </ul>
    </div>
</div>

